{
  "name": "qbead-website",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "codedocs:clean": "make clean",
    "codedocs:spinwearables": "make REPO_URL=https://github.com/qbead/SpinWearablesFirmware SRC_DIR=src",
    "codedocs:literate": "make REPO_URL=https://github.com/qbead/qbead-website SRC_DIR=tools",
    "codedocs": "bun run codedocs:clean && bun run codedocs:spinwearables && bun run codedocs:literate",
    "build": "bun run codedocs && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "devDependencies": {
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@nvl/sveltex": "^0.4.4",
    "@skeletonlabs/skeleton": "^3.2.0",
    "@skeletonlabs/skeleton-svelte": "^1.5.1",
    "@sveltejs/adapter-auto": "6.1.0",
    "@sveltejs/kit": "^2.37.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "hastscript": "^9.0.1",
    "mathjax-full": "^3.2.2",
    "mdsvex": "^0.12.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-image-toolkit": "^1.0.4",
    "rehype-katex-svelte": "^1.2.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-directive": "^4.0.0",
    "remark-math": "3.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-retext": "^6.0.1",
    "shiki": "^3.12.1",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vite": "^7.1.4"
  },
  "dependencies": {
    "@lucide/svelte": "^0.542.0",
    "@qbead/bloch-sphere": "^0.2.0",
    "@sveltejs/adapter-static": "^3.0.9",
    "three": "0.180.0"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
